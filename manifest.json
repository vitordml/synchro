{
    "manifest_version": 2,
    "name": "Synchro",
    "version": "1.0",
    "permissions": [
      "tabs",
      "storage",
      "<all_urls>"
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    }
}